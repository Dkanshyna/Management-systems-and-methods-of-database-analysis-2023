USE WebStor;
GO 
CREATE SCHEMA KANSHYNA;
GO
CREATE TABLE KANSHYNA.DARIA
(
         ID BIGINT,
		 AGE TINYINT);

 GO

  INSERT INTO KANSHYNA.DARIA(ID, AGE)
VALUES(10000, 34);

INSERT INTO KANSHYNA.DARIA(ID, AGE)
VALUES(10001, 32);

-- Implicit data type conversion
INSERT INTO KANSHYNA.DARIA(ID, AGE)
VALUES('10002', 32);

-- Implicit data type conversion
INSERT INTO KANSHYNA.DARIA(ID, AGE)
VALUES('10003', '35');

-- Implicit data type conversion
INSERT INTO KANSHYNA.DARIA(ID, AGE)
VALUES('19700101', '35');



SELECT * 
  FROM KANSHYNA.DARIA;



